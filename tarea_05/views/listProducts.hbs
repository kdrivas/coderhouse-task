<h1>Products</h1>
<table class="table">
  <thead>
    <th scope="col">Title</th>
    <th scope="col">Price</th>
    <th scope="col">Thumbnail</th>
  </thead>
  {{#each products}}
    <tr>
      <td>{{this.title}}</td>
      <td>{{this.price}}</td>
      <td><img src={{this.thumbnail}} alt={{this.thumbnail}} style="width:40px;height:40px;"/></td>
    </tr>
  {{/each}}
</table>
<a href="/api/productos/addProduct"><button>Add product</button></a>
